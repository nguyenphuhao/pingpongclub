{
  "name": "api-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:reset": "prisma migrate reset && yarn prisma:seed",
    "db:studio": "prisma studio",
    "setup": "chmod +x scripts/setup-local.sh && ./scripts/setup-local.sh",
    "mock:server": "tsx mock-services/notification-server.ts",
    "mock:test": "chmod +x scripts/test-mock-server.sh && ./scripts/test-mock-server.sh",
    "dev:all": "concurrently \"yarn mock:server\" \"yarn dev\" --names \"mock,next\" --prefix-colors \"cyan,green\"",
    "sync:admin-schema": "node scripts/sync-admin-schema.js"
  },
  "dependencies": {
    "@dokifree/database": "*",
    "@dokifree/auth": "*",
    "@sendgrid/mail": "^8.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.0.0",
    "firebase-admin": "^12.0.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.30.3",
    "twilio": "^5.10.6",
    "yaml": "^2.8.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "^14.2.0",
    "express": "^5.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
